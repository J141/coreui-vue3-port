{
  "name": "coreui-vue3-port",
  "version": "0.0.1",
  "description": "Vue 3 port of the @coreui/vue 3.2.7 package.",
  "author": "J141",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm-run-all build:default",
    "lint": "vue-cli-service lint",
    "build:default": "vue-cli-service build --target lib --name coreui-vue ./src/index.js",
    "build:es": "rollup -c --environment TARGET:esm",
    "changelog": "auto-changelog --template changelog-template.hbs --commit-limit false --package --backfill-limit 0 --starting-version 3.0.1",
    "clearCache": "jest --clearCache",
    "generate-hints": "node build/buildVscodeHints.js",
    "release": "npm-run-all clearCache lint build test:coverage generate-hints",
    "test": "vue-cli-service test:unit",
    "test:coverage": "vue-cli-service test:unit --coverage"
  },
  "main": "dist/coreui-vue.common.js",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@coreui/icons": "^2.0.0-rc.0",
    "@coreui/icons-vue": "^1.3.5",
    "@coreui/utils": "^1.3.1",
    "@popperjs/core": "^2.6.0",
    "core-js": "^3.6.5",
    "lodash.clonedeep": "~4.5.0",
    "perfect-scrollbar": "~1.5.0",
    "popper.js": "^1.16.1",
    "tooltip.js": "~1.3.3",
    "vue": "^3.0.5",
    "vue-functional-data-merge": "~3.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^5.0.0-alpha.5",
    "@vue/cli-service": "^5.0.0-alpha.5",
    "@vue/compiler-sfc": "^3.0.0-beta.1",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-alpha.1",
    "auto-changelog": "^2.2.1",
    "babel-eslint": "~10.1.0",
    "babel-jest": "~26.6.3",
    "core-js": "~3.8.1",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "~4.14.1",
    "node-sass-magic-importer": "~5.3.2",
    "npm-run-all": "~4.1.5",
    "rollup": "^2.35.1",
    "rollup-plugin-babel": "~4.4.0",
    "rollup-plugin-commonjs": "~10.1.0",
    "rollup-plugin-filesize": "~9.1.0",
    "rollup-plugin-json": "~4.0.0",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-node-resolve": "~5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-replace": "~2.2.0",
    "rollup-plugin-vue": "~5.1.9",
    "sass-loader": "^9.0.3",
    "typescript": "^4.2.3",
    "vue-router": "^4.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript",
      "@vue/airbnb",
      "@vue/typescript/recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": 2020
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie > 0"
  ],
  "bugs": {
    "url": "https://github.com/j141/coreui-vue3-port/issues"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6"
  },
  "homepage": "https://github.com/j141/coreui-vue3-port#readme",
  "keywords": [
    "coreui",
    "vue",
    "vue3",
    "vue-component",
    "vue-library",
    "bootstrap",
    "framework",
    "responsive",
    "layout",
    "component",
    "components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/j141/coreui-vue3-port.git"
  },
  "sideEffects": [
    "./src/components/toast/CToast.vue",
    "./src/components/scrollbar/CScrollbar.vue",
    "./src/components/alert/CAlert.vue",
    "./src/components/table/CDataTable.vue",
    "./src/components/tabs/CTab.vue",
    "./src/components/toast/CCarousel.vue",
    "./src/directives/old-popover.css",
    "./src/directives/old-tooltip.css"
  ],
  "types": "src/index.d.ts",
  "vetur": {
    "tags": "dist/vetur/coreui-tags.json",
    "attributes": "dist/vetur/coreui-attributes.json"
  }
}
